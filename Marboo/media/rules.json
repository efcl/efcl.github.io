{
    "isDevMode": true,
    "rule": [{
	"type": "Text",
	"extension": ["txt"],
	"mime": "text",
	"convert": [{
	    "name": "highlight",
	    "is_internal": true,
	    "output_file_extension": "html"
	},{
	    "name": "origin",
	    "is_internal": true,
	    "output_file_extension": "*"
	},{
	    "name": "marbooTemplate",
	    "is_template": true,
	    "output_file_extension": "html",
	    "path": "views/marboo.html"
	},{
	    "name": "highlightTemplate",
	    "is_template": true,
	    "output_file_extension": "html",
	    "path": "views/highlight.html"
	},{
	    "name": "k2k",
	    "is_internal": true,
	    "output_file_extension": "txt"
	}],
	"mapper": [{
	    "type": "*",
	    "init": "starts/default",
	    "router": ["highlight", "highlightTemplate"]
	},{
	    "type": "txt",
	    "router": ["md2html", "marbooTemplate"]
	},{
	    "type": "plist",
	    "router": ["highlight", "highlightTemplate"]
	},{
	    "type": "k2k.txt",
	    "router": ["k2k", "highlight", "highlightTemplate"]
	},{
	    "type": "sh",
	    "init": "starts/default.sh"
	},{
	    "type": "py",
	    "init": "starts/default.py"
	}]
    },{
	"type": "Image",
	"mime": "image",
	"convert": [{
	    "name": "displayImage",
	    "is_internal": 1,
	    "output_file_extension": "html"
	}],
	"mapper": [{
	    "type": "*",
	    "router": ["displayImage", "marbooTemplate"]
	}]
    },{
	"type": "HTML",
	"extension": ["htm", "html"],
	"mapper": [{
	    "type": "*",
	    "init": "starts/default.html",
	    "router": ["origin"]
	}]
    },{
	"type": "Markdown",
	"extension": ["md", "markdown"],
	"convert": [{
	    "name": "md2html",
	    "is_internal": 1,
	    "output_file_extension": "html"
	}],
	"mapper": [{
	    "type": "*",
	    "router": ["md2html", "marbooTemplate"]
	}]
    },{
	"type": "reStructuredText",
	"extension": ["rst"],
	"convert": [{
	    "name": "rst2html",
	    "is_internal": 1,
	    "output_file_extension": "html"
	}],
	"mapper": [{
	    "type": "*",
	    "init": "starts/default.rst",
	    "router": ["rst2html", "marbooTemplate"]
	}]
    },{
	"type": "ZIP",
	"extension": ["zip"],
	"convert": [{
	    "name": "zipList",
	    "path": "converts/zip-list.sh"
	}],
	"mapper": [{
	    "type": "*",
	    "router": ["zipList", "highlight", "marbooTemplate"]

	}]
    },{
	"type": "System UI",
	"subtype": ["alert"],
	"convert": [{
	    "name": "showAlert",
	    "path": "converts/alert.sh"
	},{
	    "name": "alertTemplate",
	    "is_template": true,
	    "path": "views/alert.html"
	}],
	"mapper": [{
	    "type": "alert",
	    "router": ["showAlert", "alertTemplate"]
	}]
    }]
}
